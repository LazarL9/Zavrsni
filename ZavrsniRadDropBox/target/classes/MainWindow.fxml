<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zavrsniraddropbox.controller.MainController">
   <children>
      <BorderPane>
         <top>
            <fx:include fx:id="menuBar" source="MenuWindow.fxml" />
         </top>
         <center>
            <VBox alignment="CENTER" prefHeight="378.0" prefWidth="600.0" spacing="20.0">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <children>
                  <Label fx:id="welcomeText" />
                  <Button onAction="#onHelloButtonClick" text="Prijava!" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
