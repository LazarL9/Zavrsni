<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="160.0" prefWidth="234.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zavrsniraddropbox.controller.MoveWindowController">
    <children>
        <BorderPane prefHeight="232.0" prefWidth="267.0">
            <center>
                <TreeView fx:id="treeView" prefHeight="218.0" prefWidth="267.0" BorderPane.alignment="CENTER" />
            </center>
            <bottom>
                <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="buttonConfirm" mnemonicParsing="false" onAction="#buttonClick" text="Potvrdi">
                     <HBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </HBox.margin></Button>
                        <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#cancelClick" text="Odustani">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" top="10.0" />
                     </HBox.margin></Button>
                    </children>
                </HBox>
            </bottom>
            <top>
                <Label BorderPane.alignment="CENTER" />
            </top>
        </BorderPane>
    </children>

</AnchorPane>
