<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="160.0" prefWidth="234.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zavrsniraddropbox.CopyWindowController">
    <children>
      <BorderPane prefHeight="232.0" prefWidth="267.0">
         <center>
            <TreeView fx:id="treeView" prefHeight="185.0" prefWidth="267.0" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <HBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                    <Button fx:id="buttonConfirm" mnemonicParsing="false" onAction="#buttonClick" text="Confirm" />
                    <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#cancelClick" text="Cancel" />
               </children>
            </HBox>
         </bottom>
         <top>
            <Label BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
    </children>
</AnchorPane>
