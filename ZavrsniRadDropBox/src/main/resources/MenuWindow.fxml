<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="11.0" prefWidth="189.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zavrsniraddropbox.controller.MenuWindowController">
    <MenuBar fx:id="mainMenu" prefHeight="25.0" prefWidth="600.0">
        <menus>
            <Menu mnemonicParsing="false" text="Datoteka">
                <items>
               <MenuItem fx:id="files" mnemonicParsing="false" onAction="#showWindowFiles" text="Datoteke" />
                </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Prijava">
                <items>
               <MenuItem fx:id="login" mnemonicParsing="false" onAction="#showMainWindow" text="Prijava" />
                </items>
            </Menu>
        </menus>
    </MenuBar>

</AnchorPane>
